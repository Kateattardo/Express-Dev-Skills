<!DOCTYPE html>
<html lang="en">
<head>
    <title><%= Skills %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Skills</h1>
      <ul>
       <% skills.forEach(function(s) { %>
        <form action="/skills/<%= s.id %>?_method=DELETE"
          class="delete-form" method="POST">
          <button type="submit">X</button>
         </form>
           <%= s.skill %>
             - 
           <%= s.level %>
         </li>
       <% }); %>
     </ul>
     <a href="/skills/new">Add New Skill</a>
     <h3>Skills as of <%= time %></h3>
  </body>
</html>
    
</body>
</html>